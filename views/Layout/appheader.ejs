            <img class="round user-select-none" alt="avatar" height="35" width="35" onerror="this.src='/app-assets/images/portrait/small/avatar.png';" src="/app-assets/images/portrait/small/avatar.png" />
            <% } %>

            <!-- Profile Company -->
