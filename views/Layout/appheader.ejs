            <span class="user-name fw-bolder">

              <!-- Administrator -->
              <% if (use.priority == 1) { %>
              <%= use.firstname %><%= use.lastname %>
              <% } %>

              <!-- Company -->
              <% if (use.priority == 2) { %>
              <%= use.lastname %>
              <% } %>

              <!-- User -->
              <% if (use.priority >= 3) { %>
              <%= use.firstname %> <%= use.lastname %>
              <% } %>

            </span>
            <span class="user-status"><%= use.username.toUpperCase() %> - <%= use.description %></span>
          </div>
          <span class="avatar">
